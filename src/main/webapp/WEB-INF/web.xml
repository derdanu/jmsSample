<web-app
        version="3.0"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
             <display-name>JMS Sample Web Application</display-name>
             <servlet>
               	<servlet-name>simpleJms</servlet-name>
               	<servlet-class>com.tsystems.appagile.poc.servlet.SimpleJms</servlet-class>
             </servlet>
             <servlet-mapping>
               	<servlet-name>simpleJms</servlet-name>
               	<url-pattern>/simpleJms</url-pattern>
             </servlet-mapping>
             <welcome-file-list>
               <welcome-file>index.jsp</welcome-file>
             </welcome-file-list> 
</web-app>